// Types and methods
type Attribute[Msg]

pub(all) enum AutoComplete {
  On
  Off
}

pub(all) enum InputType {
  Button
  Checkbox
  Color
  Date
  DateTimeLocal
  Email
  File
  Hidden
  Image
  Month
  Number
  Password
  Radio
  Range
  Reset
  Search
  Submit
  Tel
  Text
  Time
  Url
  Week
}

type Mouse
impl Mouse {
  client_pos(Self) -> Pos
  offset_pos(Self) -> Pos
  screen_pos(Self) -> Pos
}

pub(all) struct Pos {
  x : Int
  y : Int
}

type T[Msg]
impl T {
  map[A, B](Self[A], (A) -> B) -> Self[B]
  to_virtual_dom[Msg](Self[Msg]) -> @vdom.Node[Msg]
}

pub(all) enum Target {
  Self
  Blank
}

// Type aliases
pub typealias Html[Msg] = T[Msg]

