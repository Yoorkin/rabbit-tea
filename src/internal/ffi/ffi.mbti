// Values
fn current_url() -> String

fn document() -> Document

fn history_go_back() -> Unit

fn history_go_forward() -> Unit

fn load_url(String) -> Unit

fn patch(Element) -> Unit

fn push_url(String) -> Unit

fn reload_url() -> Unit

